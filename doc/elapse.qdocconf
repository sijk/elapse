project = Elapse
description = Real-time cognitive state monitoring

headerdirs = ../include ../src
sourcedirs = . ../include ../src

sources.fileextensions = "*.cpp *.qdoc"
headers.fileextensions = "*.h"

outputdir      = $PWD/html
outputformats  = HTML
outputencoding = UTF-8
sourceencoding = UTF-8

indexes = /opt/Qt/5.1.1/gcc_64/doc/qtcore/qtcore.index

include(html-template.qdocconf)
include(qt-cpp-ignore.qdocconf)

